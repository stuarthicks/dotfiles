alias bec='bundle exec cucumber'
alias bes='bundle exec spork'
alias bi='bundle install'
alias bonsai='tree -F --filelimit 15'
alias indexing='sudo mdutil -a -v -i'
alias mci='mvn clean install'
alias new_iterm='open -n -a "iTerm"'
alias p='ps aux | grep'
alias sup='svn up'
alias unused-steps='bash -c '"'"'vim --cmd "set errorformat=%m\ \#\ %f:%l" -q <( bundle exec cucumber --dry-run --format=usage | grep -B1 -i "not matched by any steps" )'"'"''
alias v='mvim -v'
alias vv='reattach-to-user-namespace mvim -g'
alias enable_ssh='sudo launchctl load -w /System/Library/LaunchDaemons/ssh.plist'
alias tcopy='tmux saveb -| pbcopy && tmux deleteb'
