source sol.light

red="\033[1;31m"
norm="\033[0;39m"
cyan="\033[1;36m"

solarized_green="\033[0;32m"
solarized_red="\033[0;31m"
solarized_blue="\033[0;34m"
solarized_yellow="\033[0;33m"

settitle () {
    printf "\033k$1\033\\"
}

ssh () {
    settitle "$*"
    ssh "$@"
}

path () {
    echo $PATH | tr : $'\n'
}

