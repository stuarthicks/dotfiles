[credential]
	helper = osxkeychain
[core]
	pager = less
	symlinks = true
[branch]
	autosetuprebase = always
[push]
	default = current
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[alias]
	abandon-ship = reset --hard HEAD^^
	br = branch
	ci = commit --verbose
	co = checkout
	down = pull
	fe = fetch --all --tags --prune
	s = status --short
	up = push
